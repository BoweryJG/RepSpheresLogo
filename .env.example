# Supabase Configuration
REACT_APP_SUPABASE_URL=https://your-project.supabase.co
REACT_APP_SUPABASE_ANON_KEY=your-anon-key-here

# Stripe Configuration
VITE_STRIPE_PUBLISHABLE_KEY=pk_test_your-stripe-publishable-key
STRIPE_SECRET_KEY=sk_test_your-stripe-secret-key
STRIPE_WEBHOOK_SECRET=whsec_your-webhook-secret

# Stripe Price IDs
STRIPE_PRICE_CLOSER_MONTHLY=price_closer_monthly_id
STRIPE_PRICE_CLOSER_ANNUAL=price_closer_annual_id
STRIPE_PRICE_DOMINATOR_MONTHLY=price_dominator_monthly_id
STRIPE_PRICE_DOMINATOR_ANNUAL=price_dominator_annual_id
STRIPE_PRICE_ENTERPRISE_MONTHLY=price_enterprise_monthly_id
STRIPE_PRICE_ENTERPRISE_ANNUAL=price_enterprise_annual_id

# Optional: Override for different environments
REACT_APP_API_URL=http://localhost:3001

# OpenRouter API Configuration
OPENROUTER_API_KEY=your-openrouter-api-key-here

# Local Claude 4 Configuration (optional)
# Set to true to use local Claude 4 instead of OpenRouter
REACT_APP_USE_LOCAL_CLAUDE=false
# If you have a local Claude API endpoint
REACT_APP_LOCAL_CLAUDE_ENDPOINT=http://localhost:8080/claude
# Optional API key for local Claude
REACT_APP_LOCAL_CLAUDE_KEY=your-local-key

# Intelligence Mode Configuration
# Streamlined Mode is now DEFAULT (Brave + Claude 4 Opus) - RECOMMENDED
# To disable and use legacy multi-model: REACT_APP_USE_STREAMLINED=false

# Legacy Super Intelligence Mode (optional)
# Only used if streamlined is disabled
# Uses Perplexity + GPT-4 + Claude 4 (more expensive, not recommended)
REACT_APP_USE_SUPER_INTELLIGENCE=false

# API Keys for Research
BRAVE_API_KEY=your-brave-api-key
FIRECRAWL_API_KEY=your-firecrawl-api-key
PERPLEXITY_API_KEY=your-perplexity-key